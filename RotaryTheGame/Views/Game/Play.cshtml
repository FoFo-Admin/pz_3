    function buildVote() {
        document.getElementById("voting").innerHTML = ``;

        let htmlBlock = `
        <div class="poll">
            <div class="poll-header">
                <div class="ordinary-blue-text">Проголосувало: ${voteResults.votesCount}/${voteResults.votable}</div>
                <div class="ordinary-blue-text">${voteResults.votesCount < voteResults.votable ? "Голосування триває" : "Результат голосування: " + (voteResults.result == "_" ? "Нічия! Усі залишаються!" : voteResults.result + " вигнали!")} </div>
            </div>
        `

        for (let key in voteResults.votes) {
            htmlBlock += `
                <div class="vote-item">
                    <div class="ordinary-blue-text">${voteResults.votes[key].name}</div>
                    <div class="ordinary-blue-text">${voteResults.votes[key].votes.length} голос / ${voteResults.votes[key].votes.length == 0 ? 0 : ((voteResults.votes[key].votes.length / voteResults.votesCount) * 100).toFixed(2)}%</div>
                    <div class="progress-bar" style="width: ${voteResults.votes[key].votes.length == 0 ? 1 : ((voteResults.votes[key].votes.length / voteResults.votesCount) * 100).toFixed(2)}%"></div>
                    <div class="itemcard-text">(${voteResults.votes[key].votes.map(value => value == "_" ? "Глядачі" : value).join(", ")})</div>
                </div>
            `
        }
        htmlBlock += `</div>`
        document.getElementById("voting").innerHTML = htmlBlock;
    }



    async function connectToServer() {
        connection = new signalR.HubConnectionBuilder()
            .withUrl("/gamesHub?code=@ViewData["Code"]")
            .build();

        connection.on("UpdateDeck", function (message) {
            me = message;
            buildMe();
        });

        connection.on("UpdateTable", function (message) {
            table = message;
            buildTable();
        });

        connection.on("UpdateVote", function (message) {
            voteResults = message;
            buildVote();
        });

        await connection.start();
    }

    buildMe();
    buildTable();

    if (voteResults != null)
        buildVote();

    connectToServer();

</script>